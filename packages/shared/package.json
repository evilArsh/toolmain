{
  "name": "@toolmain/shared",
  "version": "1.2.4",
  "description": "utility toolkit collections",
  "license": "MIT",
  "type": "module",
  "author": "arsh <https://github.com/evilArsh>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evilArsh/toolmain.git",
    "directory": "packages/shared"
  },
  "scripts": {
    "build": "rollup --config=rollup.config.ts --configPlugin=rollup-plugin-esbuild"
  },
  "keywords": [
    "vue",
    "element-plus",
    "composition api",
    "pinia",
    "dexie",
    "shiki"
  ],
  "exports": {
    ".": "./index.mjs",
    "./*": "./*"
  },
  "main": "./index.mjs",
  "module": "./index.mjs",
  "types": "./index.d.mts",
  "files": [
    "*.d.mts",
    "*.js",
    "*.mjs"
  ],
  "dependencies": {
    "csstype": "catalog:tools",
    "hotkeys-js": "catalog:tools",
    "lodash.merge": "catalog:tools",
    "nanoid": "catalog:tools",
    "rfdc": "catalog:tools",
    "serialize-error": "catalog:tools",
    "shiki": "catalog:tools",
    "style-value-types": "catalog:tools"
  },
  "devDependencies": {
    "@types/hast": "catalog:tools",
    "@types/lodash.merge": "catalog:tools",
    "@vueuse/core": "catalog:vue",
    "axios": "catalog:tools",
    "element-plus": "catalog:vue",
    "vue": "catalog:vue",
    "vue-i18n": "catalog:vue"
  },
  "peerDependencies": {
    "@vueuse/core": "catalog:vue",
    "axios": "catalog:tools",
    "element-plus": "catalog:vue",
    "vue": "catalog:vue",
    "vue-i18n": "catalog:vue"
  }
}
