{
  "name": "@toolman/element",
  "version": "1.0.0",
  "description": "element-plus toolkit",
  "license": "MIT",
  "type": "module",
  "author": "arsh <https://github.com/evilArsh>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evilArsh/toolman.git",
    "directory": "packages/shared"
  },
  "scripts": {
    "build": "rollup --config=rollup.config.ts --configPlugin=rollup-plugin-esbuild"
  },
  "keywords": [
    "vue",
    "element-plus"
  ],
  "exports": {
    ".": "./index.mjs",
    "./*": "./*"
  },
  "main": "./index.mjs",
  "module": "./index.mjs",
  "types": "./index.d.mts",
  "files": [
    "*.d.mts",
    "*.js",
    "*.mjs"
  ],
  "dependencies": {
    "@toolman/shared": "workspace:^",
    "@toolman/libs": "workspace:^"
  },
  "peerDependencies": {
    "element-plus": "^2.10.0",
    "vue": "^3.5.0"
  }
}
